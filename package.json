{
  "name": "js-ultimate",
  "version": "1.0.5",
  "description": "List of JavaScript methods which you can use natively",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "js-ultimate",
    "lodash"
  ],
  "repository": {
    "url": "https://github.com/hoatepdev/js-ultimate"
  },
  "author": "hoatepdev",
  "private": false,
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "check": "yarn lint && yarn test && yarn build",
    "build": "yarn tsc --noEmit && yarn rollup -c",
    "link": "yarn build && yarn link",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" --ignore-unknown"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.11",
    "esbuild": "^0.19.11",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-node-externals": "^6.1.2",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
